    "v1": {
      "send": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "temp": {
            "type": "number"
          },
          "msg": {
            "type": "string"
          }
        },
        "required": ["msg","temp"]
      },
      "receive": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "temp": {
            "type": "number"
          },
          "msg": {
            "type": "string"
          }
        },
        "required": ["msg","temp"]
      }
    }
  }
}