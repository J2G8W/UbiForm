{
  "name": "Publisher",
  "url": "tcp://127.0.0.1:8000",
  "schemas": {
    "v1": {
      "send": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "recursiveObject": {
            "type": "object",
            "properties": {
              "time": {
                "type": "number"
              },
              "date": {
                "type": "string"
              }
            }
          },
          "msgArray": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["recursiveObject"]
      }
    }
  }
}