{
  "name": "PublisherExample",
  "urls": ["tcp://127.0.0.1"],
  "port": 8000,
  "schemas": {
    "publisherExample": {
      "socketType": "publisher",
      "send": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "recursiveObject": {
            "type": "object",
            "properties": {
              "time": {
                "type": "number"
              },
              "date": {
                "type": "string"
              }
            }
          },
          "msgArray": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["recursiveObject"]
      }
    }
  }
}