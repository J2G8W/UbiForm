{
  "name": "Pair",
  "schemas": {
    "pairEvaluation": {
      "connectionParadigm": "pair",
      "send": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "stringProperty" : {
            "type" : "string"
          },
          "numberProperty" : {
            "type" : "number"
          },
          "objectProperty" : {
            "type" : "object",
            "properties" : {
              "subString" : {
                "type" : "string"
              }
            },
            "required" : ["subString"]
          }
        },
        "required" : ["stringProperty", "numberProperty", "objectProperty"]
      },
      "receive": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "stringProperty" : {
            "type" : "string"
          },
          "numberProperty" : {
            "type" : "number"
          },
          "objectProperty" : {
            "type" : "object",
            "properties" : {
              "subString" : {
                "type" : "string"
              }
            },
            "required" : ["subString"]
          }
        },
        "required" : ["stringProperty", "numberProperty", "objectProperty"]
      }
    }
  }
}